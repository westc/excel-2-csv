{
  "name": "Excel2CSV-Converter",
  "version": "1.0.0",
  "description": "Convert Excel workbooks to individual CSVs in bulk.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . \"Excel-2-CSV Converter\" --version=0.36.8",
    "build-mac32": "electron-packager . \"Excel-2-CSV Converter\" --version=0.36.8 --platform=darwin --arch=ia32",
    "build-mac64": "electron-packager . \"Excel-2-CSV Converter\" --version=0.36.8 --platform=darwin --arch=x64",
    "build-win32": "electron-packager . \"Excel-2-CSV Converter\" --version=0.36.8 --platform=win32 --arch=ia32",
    "build-win64": "electron-packager . \"Excel-2-CSV Converter\" --version=0.36.8 --platform=win32 --arch=x64"
  },
  "author": "Christopher West",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.8",
    "mkdirp": "^0.5.1",
    "xlsx": "^0.8.2"
  }
}
